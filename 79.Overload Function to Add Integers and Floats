#include <iostream>

class Adder {
public:
    int add(int a, int b) {
        return a + b;
    }

    double add(double a, double b) {
        return a + b;
    }
};

int main() {
    Adder adder;
    std::cout << "2 + 3 = " << adder.add(2, 3) << std::endl;
    std::cout << "2.5 + 3.5 = " << adder.add(2.5, 3.5) << std::endl;
    return 0;
}
