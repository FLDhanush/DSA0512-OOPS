#include <iostream>

class MaxFinder {
public:
    int max(int a, int b) {
        return (a > b)? a : b;
    }

    double max(double a, double b) {
        return (a > b)? a : b;
    }

    char max(char a, char b) {
        return (a > b)? a : b;
    }
};

int main() {
    MaxFinder finder;
    std::cout << "Max of 2 and 3: " << finder.max(2, 3) << std::endl;
    std::cout << "Max of 2.5 and 3.5: " << finder.max(2.5, 3.5) << std::endl;
    std::cout << "Max of 'a' and 'b': " << finder.max('a', 'b') << std::endl;
    return 0;
}
