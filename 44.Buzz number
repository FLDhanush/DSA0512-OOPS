#include <iostream>
using namespace std;

bool isBuzz(int num) {
    return (num % 7 == 0) || (num % 10 == 7);
}

int main() {
    int n;
    cout << "Enter a number: ";
    cin >> n;

    if (isBuzz(n))
        cout << n << " is a Buzz number." << endl;
    else
        cout << n << " is not a Buzz number." << endl;

    return 0;
}
